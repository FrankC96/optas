<?xml version="1.0" ?>
<robot name="test_robot">

<link name="world"/>
<link name="link1"/>
<link name="link2"/>
<link name="link3"/>
<link name="eff"/>

<joint name="joint0" type="continuous">
  <origin xyz="0 0 0"/>
  <axis xyz="0 0 1"/>
  <parent link="world"/>
  <child link="link1"/>
</joint>

<joint name="joint1" type="revolute">
  <origin xyz="2 0 0"/>
  <axis xyz="0 0 1"/>
  <limit lower="-1" upper="1" velocity="1" effort="100"/>
  <parent link="link1"/>
  <child link="link2"/>
</joint>

<joint name="joint3" type="prismatic">
  <origin xyz="1 0 0"/>
  <axis xyz="0 0 1"/>
  <limit lower="0" upper="1" velocity="1" effort="100"/>
  <parent link="link2"/>
  <child link="link3"/>
</joint>

<joint name="eff_joint" type="fixed">
  <origin xyz="0 0 0.5"/>
  <parent link="link3"/>
  <child link="eff"/>
</joint>

</robot>
